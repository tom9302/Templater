{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/?(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/",
      "dest": "server.js"
    }
  ],
  "functions": {
    "server.js": {
      "memory": 3008,
      "maxDuration": 10
    }
  }
}